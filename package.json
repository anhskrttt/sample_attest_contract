{
  "name": "nft-japan-contract",
  "version": "1.0.0",
  "description": "nft japan smart contract",
  "scripts": {
    "compile": "yarn hardhat compile",
    "console": "yarn hardhat console --no-compile",
    "deploy": "yarn hardhat run scripts/deploy.js",
    "deploy:rinkeby": "yarn hardhat run scripts/deploy.js --network rinkeby",
    "deploy:tomotestnet": "yarn hardhat run scripts/deploy-uups.js --network tomotestnet",
    "deploy:tomomainnet": "yarn hardhat run scripts/deploy-uups.js --network tomomainnet",
    "test": "yarn hardhat test",
    "start:test": "node ./app/index.js",
    "start:dev": "nodemon ./app/index.js",
    "start:production": "node ./app/index.js",
    "start": "node ./app/index.js",
    "start:staging": "node ./app/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://git.jetbrains.space/blockchainlab/nft-japan/NFT-JAPAN-CONTRACT.git"
  },

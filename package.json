    "@openzeppelin/hardhat-upgrades": "^1.11.0",
    "axios": "^0.21.1",
    "dotenv": "^9.0.2",
    "events": "^3.3.0",
    "express": "^4.17.1",
    "global": "^4.4.0",
    "globby": "^11.0.3",
    "hardhat": "^2.3.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.9"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.0",
